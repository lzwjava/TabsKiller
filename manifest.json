{
  "manifest_version": 2,

  "name": "Tabs Killer",
  "description": "This extension automatically kill the oldest tabs when tabs are too many.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "background.html"
  },
  "permissions": [
    "tabs", "storage"
  ]
}
